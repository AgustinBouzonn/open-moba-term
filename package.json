{
  "name": "openmoba",
  "productName": "OpenMoba",
  "version": "1.0.0",
  "description": "High-performance multiprotocol session manager for SSH, SFTP, VNC, and RDP",
  "homepage": "https://github.com/agustinbouzonn/open-moba-term",
  "repository": {
    "type": "git",
    "url": "https://github.com/agustinbouzonn/open-moba-term.git"
  },
  "bugs": {
    "url": "https://github.com/agustinbouzonn/open-moba-term/issues"
  },
  "main": "dist-electron/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:win": "tsc && vite build && electron-builder --win",
    "build:mac": "tsc && vite build && electron-builder --mac",
    "build:linux": "tsc && vite build && electron-builder --linux",
    "dist": "npm run build",
    "dist:dir": "tsc && vite build && electron-builder --dir",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "patch-package && electron-builder install-app-deps"
  },
  "keywords": [
    "electron",
    "ssh",
    "terminal",
    "sftp",
    "rdp",
    "vnc"
  ],
  "author": {
    "name": "Agustin Bouzon",
    "email": "agustinbouzon10@gmail.com"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "keytar": "^7.9.0",
    "lucide-react": "^0.562.0",
    "node-pty": "^1.0.0",
    "node-rdpjs": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rfb2": "^0.2.2",
    "ssh2": "^1.15.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-webgl": "^0.16.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/ssh2": "^1.11.19",
    "@types/xterm": "latest",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "patch-package": "^8.0.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
